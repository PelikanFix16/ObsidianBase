{
  "settings": {
    "migrated": true,
    "iconPacksPath": ".obsidian/plugins/obsidian-icon-folder/icons",
    "fontSize": 16,
    "emojiStyle": "none",
    "iconColor": null,
    "recentlyUsedIcons": [],
    "recentlyUsedIconsSize": 5,
    "rules": [
      {
        "rule": "./*",
        "icon": "FarFolder",
        "for": "folders",
        "color": "#43ccf9"
      },
      {
        "rule": "\\d{4}-\\d{2}-\\d{2}",
        "icon": "FarCalendarPlus",
        "for": "folders"
      },
      {
        "rule": "Tasks",
        "icon": "FarSquareCheck",
        "for": "folders"
      }
    ],
    "extraMargin": {
      "top": 0,
      "right": 4,
      "bottom": 0,
      "left": 0
    },
    "iconInTabsEnabled": false
  }
}